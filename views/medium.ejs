      <img class="profile_img" src="<%=rss.image%>">
      <h3 class="int">Latest</h3>
      
        <% rss.items.forEach(article=>{%>
          <div class="main_inside">
            <a><img  style="height: 2.6rem; width: 2.6rem; border-radius: 50%; position: absolute;" src="<%=rss.image%>"></a>
            <div class="main_in_2">
              <a style="color: black;"><%=article.dc_creator%></a><br>
              <a><%=article.pubDate.substring(4).substring(0,7)%>, <%=article.pubDate.substring(11).substring(0,6)%> Â· <%=Math.floor(article.content_encoded.split(" ").length/130)%> min read</a>  
            </div><br><br>
          <a href="<%=article.link%>" style="text-decoration: none; color: black;"><h1><%=article.title%></h1></a> 
